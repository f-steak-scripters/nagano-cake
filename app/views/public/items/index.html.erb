<div class="container m-5">
<h4>商品一覧</h4><p>(全<%= @items_count %>件</p>
<% @items.each do |item| %>
<div>
  <%= link_to item_path(item.id) do %>
   <div>
      <%= image_tag item.image if report.image.attached? %>
      <p><%= item.name %></p>
      <p><%= item.price %></p>
   </div>
  <% end %>
</div>
<% end %>
</div>